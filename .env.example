VITE_BASE_URL=http://localhost:5000/api/v1

# Vapi AI Voice Assistant Configuration
# These are PUBLIC variables (exposed to the client-side code)
# Copy values from your Vapi dashboard and add them to .env.local

# Vapi Client ID - Get this from https://dashboard.vapi.ai
NEXT_PUBLIC_VAPI_CLIENT_ID=your_vapi_client_id_here

# Vapi Assistant ID - Get this from https://dashboard.vapi.ai
NEXT_PUBLIC_VAPI_ASSISTANT_ID=your_vapi_assistant_id_here

# Optional: Server-side Vapi API key (keep this secret, never expose to client)
# VAPI_SERVER_KEY=your_vapi_api_key_here